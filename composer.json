{
  "name": "robinmarechal/laravel-rest-api",
  "type": "library",
  "keywords": ["rest", "api", "laravel"],
  "description": "An easy way to add a REST API to your project",
  "authors": [
    {
      "name": "RobinMarechal",
      "email": "robin.marechal-2@etu.univ-tours.fr"
    }
  ],
  "require": {
    "php": "7.*",
    "laravel/framework": "~5.6"
  },
  "autoload": {
    "psr-4": {
      "RobinMarechal\\RestApi\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RobinMarechal\\RestApi\\RestApiServiceProvider"
      ]
    }
  }
}
